f1: field, type="bmap1d",
    	   magneticFile="bdsim1d:1dexample.tar.gz",
	   magneticInterpolator="cubic1d",
	   bScaling=0.5,
	   integrator="g4classicalrk4";


box: element, fieldAll="f1", l=1.1*m, geometry="gdml:abox.gdml";


l1: line=(box);

use, l1;

beam, particle="e-",
      energy=3*GeV,
      distrType="gauss",
      sigmaX=10*cm,
      sigmaY=10*cm,
      sigmaXp=1e-6,
      sigmaYp=1e-6;


option, ngenerate=10;